    "setup:dev": "chmod +x scripts/setup-dev.sh && ./scripts/setup-dev.sh",
    "deploy": "chmod +x scripts/deploy.sh && ./scripts/deploy.sh"
  },
  "dependencies": {
    "@prisma/client": "^5.7.0",
